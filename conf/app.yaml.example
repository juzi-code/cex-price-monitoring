tg:
  botToken: xxx
  spotChatID: xxx
  futuresChatID: xxx

log:
  level: info          # 日志级别: debug, info, warn, error
  format: text         # 日志格式: json, text
  output: both         # 输出方式: console, file, both
  filePath: logs/app.log  # 日志文件路径
  maxSize: 100         # 单个日志文件最大大小(MB)
  maxBackups: 3        # 保留的旧日志文件数量
  maxAge: 7            # 日志文件保留天数
  compress: true       # 是否压缩旧日志文件